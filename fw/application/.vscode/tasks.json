{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Pixl.js",
      "type": "shell",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "command": "make all -j64",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    },
    {
      "label": "Flash Pixl.js",
      "type": "shell",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "command": "make flash_ocd",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
